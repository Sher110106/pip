{
  "name": "python-dependency-resolver",
  "version": "1.0.0",
  "description": "AI-powered Python dependency resolution system with deprecation detection and intelligent version management",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite dev",
    "deploy": "vite build --mode production && wrangler deploy",
    "deploy:dev": "vite build && wrangler deploy",
    "deploy:production": "vite build --mode production && wrangler deploy",
    "test": "vitest",
    "types": "wrangler types",
    "format": "prettier --write .",
    "check": "prettier . --check && biome lint && tsc"
  },
  "keywords": [
    "python",
    "dependencies",
    "ai-agents",
    "package-management",
    "cloudflare",
    "dependency-resolution"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vite-plugin": "1.1.1",
    "@cloudflare/vitest-pool-workers": "^0.8.27",
    "@cloudflare/workers-types": "^4.20250513.0",
    "@tailwindcss/vite": "^4.1.6",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "3.1.3",
    "wrangler": "^4.16.1"
  },
  "dependencies": {
    "@ai-sdk/azure": "^1.3.23",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@gsap/react": "^2.1.2",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@types/marked": "^6.0.0",
    "@types/semver": "^7.5.8",
    "agents": "^0.0.93",
    "ai": "^4.3.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "marked": "^15.0.11",
    "octokit": "^4.1.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "semver": "^7.6.3",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.24.4"
  }
